    <aliases>
        <item name="num_nodes_with_state">
            <queryFormatString>count(//row[State=&quot;{0}&quot;])</queryFormatString>
            <arguments>
                <item name="state">
                    <valuesQuery>//State</valuesQuery>
                    <interpretAs>SampleValues</interpretAs>
                </item>
            </arguments>
        </item>
        <item name="non_reset_LC">
            <queryFormatString>mapped/Tabular/table1/table/row[not(contains(Type,&quot;RSP&quot;)) and not(contains(State,&quot;RESET&quot;)) and not(contains(Type,&quot;RP&quot;))]/Node</queryFormatString>
        </item>
        <item name="num_cards_down">
            <queryFormatString>fn:count(//State[../State!=&quot;IOS XR RUN&quot; and ../State!=&quot;OK&quot; and ../State!=&quot;UNPOWERED&quot;])</queryFormatString>
        </item>
        <item name="find_node_by_type">
            <queryFormatString>//mapped/Tabular/table1/table/row[contains(Type,&quot;{0}&quot;)]/Node</queryFormatString>
            <arguments>
                <item name="type">
                    <defaultvalue>Active</defaultvalue>
                    <valuesQuery>//Type</valuesQuery>
                    <interpretAs>SampleNodeNames</interpretAs>
                </item>
            </arguments>
        </item>
        <item name="num_rsp">
            <queryFormatString>count(mapped/Tabular/table1/table/row[contains(Type,&quot;RSP&quot;)]/Node)</queryFormatString>
        </item>
        <item name="get_LC_loc">
            <description>get the line card locatons only not RSP</description>
            <queryFormatString>mapped/Tabular/table1/table/row[not(contains(Type,&quot;RSP&quot;)) and not(contains(Type,&quot;RP&quot;)) and not(contains(Type,&quot;Slice&quot;)) and not(contains(Type,&quot;FC&quot;)) and not(contains(Type,&quot;SC&quot;)) and not(contains(Type,&quot;FAN&quot;)) ]/Node</queryFormatString>
        </item>
        <item name="get_LC_down">
            <queryFormatString>mapped/Tabular/table1/table/row[not(contains(Type,&quot;IOS  XR  RUN&quot;))]/State</queryFormatString>
        </item>
        <item name="get_Active_RSP">
            <description>get the line card locatons only not RSP</description>
            <queryFormatString>mapped/Tabular/table1/table/row[(contains(Type,&quot;Active&quot;))]/Node</queryFormatString>
        </item>
        <item name="get_Standby_RSP">
            <description>get the line card locatons only not RSP</description>
            <queryFormatString>mapped/Tabular/table1/table/row[(contains(Type,&quot;Standby&quot;))]/Node</queryFormatString>
        </item>
        <item name="get_rp_loc">
            <description>gives u the RP locations</description>
            <queryFormatString>mapped/Tabular/table1/table/row[contains(Node,&quot;RSP&quot;) or contains(Node,&quot;RP&quot;)]/Node</queryFormatString>
        </item>
    </aliases>
</ResponseMap>
