    <mapperProperties>
        <item type="com.fnfr.svt.mapping.table.TabularMapperProperties">
            <tabularMaps>
                <item name="table1">
                    <additionalBannerLineCount>1</additionalBannerLineCount>
                    <banner>Node\\s+Type\\s+State\\s+Config\\sState</banner>
                    <footer>RP/* </footer>
                    <bannerStructure>Regex</bannerStructure>
                    <footerStructure>Wildcard</footerStructure>
                    <columnBoundaries>Positional</columnBoundaries>
                    <columns>
                        <item name="Node">
                            <isKey>true</isKey>
                            <width>16</width>
                            <lastFullCell></lastFullCell>
                        </item>
                        <item name="Type">
                            <isKey>true</isKey>
                            <width>26</width>
                            <lastFullCell></lastFullCell>
                        </item>
                        <item name="State">
                            <isKey>true</isKey>
                            <width>17</width>
                            <lastFullCell></lastFullCell>
                        </item>
                        <item name="Con">
                            <width>0</width>
                            <lastFullCell></lastFullCell>
                        </item>
                    </columns>
                </item>
            </tabularMaps>
        </item>
    </mapperProperties>
