        <item name="ConfigureInterfaceIP" isPublic="true">
            <description>Configure Interface IPv4 Address</description>
            <steps>
                <item guid="ea5e3f8b-eedb-4d50-b8d4-44ed58e0240f" action="EnterConfigMode" session="$session">
                    <command>
                        <body> -interface $interface</body>
                    </command>
