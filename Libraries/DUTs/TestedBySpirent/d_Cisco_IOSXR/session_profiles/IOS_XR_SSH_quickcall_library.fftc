                    <nestedSteps>
                        <item guid="fb66fd58-9436-433a-b8b7-407d5ed09bc3" action="eval">
                            <command>
                                <body>set v4_regex {\\b((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)(\\.|$)){4}\\b}</body>
