        <item name="CreateAcl" isPublic="true">
            <description>Create ACL</description>
            <steps>
                <item guid="538612db-ea73-4d56-9cb2-b70ae4e40b51" action="EnterConfigMode" session="$session">
                    <command>
                        <body> -interface none</body>
                    </command>
                    <applicationProperties type="com.fnfr.svt.editors.terminal.documents.StepDefaultProperties"/>
                    <useFieldsInCommand>false</useFieldsInCommand>
                </item>
                <item guid="34a6fbaf-2a9e-4d00-bef3-53f0a7154dd1" action="command" session="$session">
                    <command>
                        <body>ipv4 access-list $acl_name</body>
                    </command>
                    <applicationProperties type="com.fnfr.svt.editors.terminal.documents.StepDefaultProperties"/>
                </item>
                <item guid="0d457ff6-4d3c-428d-b2dc-b6c51e994094" action="command" session="$session">
                    <command>
                        <body>1 remark ACL created by automated iTest script. ACL should be deleted upon script completion.</body>
                    </command>
                    <applicationProperties type="com.fnfr.svt.editors.terminal.documents.StepDefaultProperties"/>
                </item>
                <item guid="4e73d9c7-8077-4350-8e15-0ee736dde1cb" action="command" session="$session">
                    <command>
                        <body>2 deny $deny_string</body>
                    </command>
                    <applicationProperties type="com.fnfr.svt.editors.terminal.documents.StepDefaultProperties"/>
                </item>
                <item guid="555b94b1-86ed-4df4-bc28-f4a0815b859c" action="command" session="$session">
                    <command>
                        <body>3 permit $permit_string</body>
                    </command>
                    <applicationProperties type="com.fnfr.svt.editors.terminal.documents.StepDefaultProperties"/>
                </item>
                <item guid="5ad0cc8d-ef0d-4380-b4d1-fe310f407aff" action="ExitConfigMode" session="$session">
                    <applicationProperties type="com.fnfr.svt.editors.terminal.documents.StepDefaultProperties"/>
                    <useFieldsInCommand>false</useFieldsInCommand>
                </item>
            </steps>
            <multilineDescription>Create an Access Control List on the global config</multilineDescription>
            <arguments>
                <item name="acl_name">
                    <description>Name to give to newly created ACL</description>
                    <defaultValue>itest_acl</defaultValue>
                </item>
                <item name="deny_string">
                    <description>String to be placed on the &apos;deny&apos; line of the ACL. Example &quot;tcp any any eq www log&quot;</description>
                    <defaultValue> </defaultValue>
                </item>
                <item name="permit_string">
                    <description>String to be placed on the &apos;permit&apos; line of the ACL. Example &quot;ipv4 any any&quot;</description>
                    <defaultValue> </defaultValue>
                </item>
            </arguments>
        </item>
